<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

    <!--<properties resource="/generatorConfig.xml"/>-->

    <classPathEntry location="G:\.m2\repository\mysql\mysql-connector-java\5.1.44\mysql-connector-java-5.1.44.jar" />
    <context id="MysqlContext" targetRuntime="MyBatis3" defaultModelType="flat">
        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>
        <plugin type="org.mybatis.generator.plugins.RowBoundsPlugin"/>

        <!-- 生成注释配置 -->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="false"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/springboot" userId="root" password="root"/>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.example.demo.dao.mysql.model" targetProject="I:\ideaWorkSpace\demo\src\main\java"/>

        <sqlMapGenerator targetPackage="com.example.demo.dao.mysql.xml" targetProject="I:\ideaWorkSpace\demo\src\main\java"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.example.demo.dao.mysql.mapper" targetProject="I:\ideaWorkSpace\demo\src\main\java">
            <property name="trimStrings" value="true"/>
        </javaClientGenerator>

        <table tableName="t_department" domainObjectName="Department">
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>

        <table tableName="t_role" domainObjectName="Role">
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>

        <table tableName="t_user" domainObjectName="User">
            <generatedKey column="id" sqlStatement="Mysql" identity="true"/>
        </table>
       
    </context>

</generatorConfiguration>
